{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "declaration": true,
    "outDir": "./dist",
    "declarationDir": "./dist",
    "rootDir": ".",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "types": ["node"]
  },
  "exclude": [
    "dist",
    "**/*.spec.ts",
    "vitest.config.ts",
    "vitest.setup.ts",
    ".baserowrc",
    "__generated__"
  ]
}